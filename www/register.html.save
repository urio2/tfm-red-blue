<body class="animsition">

  <div id="navBarContainer" class="container-fluid">  
  </div>

  <!-- content page -->
  <section class="bgwhite p-t-66 p-b-60 flex-c">
    <div class="container">
      <div class="row">

        <div class="p-b-30" align="center" style="margin: auto;">
            <h4 class="m-text26 p-b-36 p-t-15">
              Intruduzca sus datos para registrarse
            </h4>
            Usuario:
            <div class="bo4 of-hidden size15 m-b-20">
              <input id="userRegister"class="sizefull s-text7 p-l-22 p-r-22" type="text" name="user" placeholder="Usuario">
            </div>
            
            Contraseña:
            <div class="bo4 of-hidden size15 m-b-20">
              <input id="passRegister" class="sizefull s-text7 p-l-22 p-r-22" type="password" name="pass" placeholder="Contraseña">
            </div>

            Tarjeta de crédito:
            <div class="bo4 of-hidden size15 m-b-20">
              <input id="creditCardRegister" class="sizefull s-text7 p-l-22 p-r-22" type="text" name="creditCard" placeholder="Tarjeta de crédito">
            </div>

            <span id="errorRegisterMsg" style="color:red;"></span>

            <div class="w-size25">
              <!-- Button -->
              <button type="button" class="flex-c-m size2 bg1 bo-rad-23 hov1 m-text3 trans-0-4" onclick="register()">
                Registrarse
              </button>

              <a href="login.html" class="s-text7">
                Cancelar
              </a>
            </div>
        </div>
      </div>
    </div>
  </section>


